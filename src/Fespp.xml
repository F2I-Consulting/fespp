 <ServerManagerConfiguration>
	<ProxyGroup name="sources">
		<!-- EPCReader -->
		<SourceProxy name="Fespp" 
			class="Fespp" 
			label="F2i-consulting Energistics Standard Paraview Plugin">
			<Documentation
				long_help="Read a .epc file."
				short_help="Read a .epc file.">
			</Documentation>
			<Hints>
				<ReaderFactory extensions="epc" file_description="EPC files" />
    			<PipelineIcon name="CINEMA_MARK"/>
  			</Hints>

<!--  Pipe Name -->
<!-- 			
			<StringVectorProperty
				name="FileName"
				animateable="0"
				command="SetFileName"
				number_of_elements="1"
				panel_visibility="advanced">
 				<FileListDomain name="files"/> 
				<Documentation>
					This property specifies the file name for the EPC reader.
				</Documentation>
			</StringVectorProperty>
			-->

<!--  File add to pipe -->

			<StringVectorProperty
				name="EpcFileName"
				animateable="0"
				command="SetSubFileName"
				number_of_elements="1"
				panel_visibility="advanced">
				<FileListDomain name="files"/>
				<Hints>
    				<FileChooser extensions="epc" file_description="EPC files" />
  				</Hints>
			</StringVectorProperty>
			
<!-- Uuid List of work: -->

			<StringVectorProperty
			        name="uuidInfo"
			        information_only="1"        
			        default = "1">
			        <ArraySelectionInformationHelper attribute_name="UuidList"/>
			</StringVectorProperty>
			<StringVectorProperty name="UuidList"
				command="SetUuidList" repeat_command="1"
				number_of_elements_per_command="2" element_types="2 0"
				information_property="uuidInfo" label="uuid to display"
				panel_visibility="never">
				<ArraySelectionDomain name="array_list">
					<RequiredProperties>
						<Property name="uuidInfo"
							function="ArrayList" />
					</RequiredProperties>
				</ArraySelectionDomain>
				<Documentation>List uuid to display</Documentation>
			</StringVectorProperty>
							
<!-- Wellbore marker properties: -->

	<!-- enable/disable Marker orientation -->
			<IntVectorProperty name="MarkerOrientation"
            	command="setMarkerOrientation"
                number_of_elements="1"
                default_values="1">
    			<BooleanDomain name="bool"/>
			</IntVectorProperty>
	<!-- marker size -->
			<IntVectorProperty name="MarkerSize"
            	command="setMarkerSize"
                number_of_elements="1"
                default_values="10">
			</IntVectorProperty>
			
			<PropertyGroup label="Wellbore marker" panel_visibility="panel_widget">
   				<Property name="MarkerOrientation" />
   				<Property name="MarkerSize" />
			</PropertyGroup>


			<!-- End EPCReader -->
		</SourceProxy>
		
	</ProxyGroup>
<!-- End Reader -->
 </ServerManagerConfiguration>
